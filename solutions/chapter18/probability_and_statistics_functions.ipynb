   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "a = normal(0,1,size=(1000,))\n",
    "b = normal(3,3,size=(1000,))\n",
    "c = uniform(0,1,size=(1000,))\n",
    "d = uniform(-1,1,size=(1000,))\n",
    "e = gamma(1,2,size=(1000,))\n",
